@article{ ISI:000324861400008,
Author = {Daniels, Karla J. and Park, Yang-Nim and Srikantha, Thyagarajan and
   Pujol, Claude and Soll, David R.},
Title = {{Impact of Environmental Conditions on the Form and Function of Candida
   albicans Biofilms}},
Journal = {{EUKARYOTIC CELL}},
