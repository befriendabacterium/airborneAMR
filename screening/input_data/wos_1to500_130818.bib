@article{ ISI:000302884800004,
Author = {Zhang, John and Levin, Robert and Angelo, Robert and Vincent, Richard
   and Brickner, Philip and Ngai, Peter and Nardell, Edward A.},
Title = {{A Radiometry Protocol for UVGI Fixtures Using a Moving-Mirror Type
   Gonioradiometer}},
