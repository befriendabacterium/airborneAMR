@article{ ISI:000361919700012,
Author = {Chitnis, Amit S. and Davis, J. Lucian and Schecter, Gisela F. and Barry,
   Pennan M. and Flood, Jennifer M.},
Title = {{Review of Nucleic Acid Amplification Tests and Clinical Prediction Rules
   for Diagnosis of Tuberculosis in Acute Care Facilities}},
