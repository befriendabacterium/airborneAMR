@article{ ISI:000425474600001,
Author = {Zhou, Hao and Wang, Xiaolong and Li, Zhaohuan and Kuang, Yu and Mao,
   Daqing and Luo, Yi},
Title = {{Occurrence and Distribution of Urban Dust-Associated Bacterial
   Antibiotic Resistance in Northern China}},
