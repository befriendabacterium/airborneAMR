@article{ ISI:000300770000006,
Author = {Augustine, Nimmy and Peter, Wilson A. and Kerkar, Savita and Thomas,
   Sabu},
Title = {{Arctic Actinomycetes as Potential Inhibitors of Vibrio cholerae Biofilm}},
Journal = {{CURRENT MICROBIOLOGY}},
