@article{ ISI:000436128700011,
Author = {Elbadawy, Mohamed and Usui, Tatsuya and Yamawaki, Hideyuki and Sasaki,
   Kazuaki},
Title = {{Development of an Experimental Model for Analyzing Drug Resistance in
   Colorectal Cancer}},
Journal = {{CANCERS}},
Year = {{2018}},
Volume = {{10}},
Number = {{6}},
Month = {{JUN}},
Abstract = {{Colorectal cancer (CRC) is one of the most common cancers, for which
   combination treatment of chemotherapy is employed. However, most
   patients develop drug resistance during the course of treatment. To
   clarify the mechanisms of drug resistance, various research models have
   been developed. Recently, we established a human CRC patients-derived
   three-dimensional (3D) culture system using an air-liquid interface
   organoid method. It contained numerous cancer stem cells and showed
   resistance to 5-fluorouracil and Irinotecan. In this review, we
   introduce conventional and our established models for studying drug
   resistance in CRC.}},
