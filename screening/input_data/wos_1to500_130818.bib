@article{ ISI:000385864800002,
Author = {Lenart-Boron, Anna and Wolny-Koladka, Katarzyna and Stec, Joanna and
   Kasprowic, Andrzej},
Title = {{Phenotypic and Molecular Antibiotic Resistance Determination of Airborne
   Coagulase Negative Staphylococcus spp. Strains from Healthcare
   Facilities in Southern Poland}},
