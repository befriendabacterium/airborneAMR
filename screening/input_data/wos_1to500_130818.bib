@article{ ISI:000334124300015,
Author = {Van Meervenne, Eva and De Weirdt, Rosemarie and Van Coillie, Els and
   Devlieghere, Frank and Herman, Lieve and Boon, Nico},
Title = {{Biofilm models for the food industry: hot spots for plasmid transfer?}},
