@article{ ISI:A1991GN19200002,
Author = {SAKATA, K and KWOK, TT and MURPHY, BJ and LADEROUTE, KR and GORDON, GR
   and SUTHERLAND, RM},
Title = {{HYPOXIA-INDUCED DRUG-RESISTANCE - COMPARISON TO
   P-GLYCOPROTEIN-ASSOCIATED DRUG-RESISTANCE}},
Journal = {{BRITISH JOURNAL OF CANCER}},
Year = {{1991}},
Volume = {{64}},
Number = {{5}},
Pages = {{809-814}},
Month = {{NOV}},
Abstract = {{In this report, we investigate several examples of hypoxia-induced drug
   resistance and compare them with P-glycoprotein associated multidrug
   resistance (MDR).  EMT6/Ro cells exposed to drugs in air immediately
   after hypoxic treatment developed resistance to adriamycin,
   5-fluorouracil, and actinomycin D.  However, these cells did not develop
   resistance to colchicine, vincristine or cisplatin.  When the cells were
   returned to a normal oxygen environment, they lost resistance.  There
   was no correlation between the content of adriamycin and the development
   of adriamycin resistance induced by hypoxia.  There was no difference
   between the efflux of adriamycin from aerobic cells and that from
   hypoxia-treated cells.  The mRNA for P-glycoprotein was not detected in
   the hypoxia-treated cells.  These results suggest that hypoxia-induced
   drug resistance is different from P-glycoprotein associated multidrug
   resistance.}},
