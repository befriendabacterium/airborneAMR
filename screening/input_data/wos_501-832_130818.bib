@article{ ISI:000081364900006,
Author = {Caya, F and Fairbrother, JM and Lessard, L and Quessy, S},
Title = {{Characterization of the risk to human health of pathogenic Escherichia
   coli isolates from chicken carcasses}},
Journal = {{JOURNAL OF FOOD PROTECTION}},
