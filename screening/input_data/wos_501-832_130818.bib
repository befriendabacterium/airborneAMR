@article{ ISI:000239278600057,
Author = {Sambhy, Varun and MacBride, Megan M. and Peterson, Blake R. and Sen,
   Ayusman},
Title = {{Silver bromide nanoparticle/polymer composites: Dual action tunable
   antimicrobial materials}},
