Title = {{Global transport networks and infectious disease spread}},
Booktitle = {{ADVANCES IN PARASITOLOGY, VOL 62: GLOBAL MAPPING OF INFECTIOUS DISEASES:
   METHODS, EXAMPLES AND EMERGING APPLICATIONS}},
