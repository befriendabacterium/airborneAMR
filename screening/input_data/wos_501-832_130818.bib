@article{ ISI:000269654500006,
Author = {Topp, E. and Scott, A. and Lapen, D. R. and Lyautey, E. and Duriez, P.},
Title = {{Livestock waste treatment systems for reducing environmental exposure to
   hazardous enteric pathogens: Some considerations}},
