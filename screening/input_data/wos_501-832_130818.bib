@article{ ISI:000265249200005,
Author = {Shah, Rena H. and Frishman, William H.},
Title = {{Adenosine(1) Receptor Antagonism A New Therapeutic Approach for the
   Treatment of Decompensated Heart Failure}},
Journal = {{CARDIOLOGY IN REVIEW}},
Year = {{2009}},
Volume = {{17}},
Number = {{3}},
Pages = {{125-131}},
Month = {{MAY-JUN}},
Abstract = {{Heart failure (HF) remains a major cause of morbidity and mortality,
   even with the use of standard treatments in patients with chronic HF and
   acute decompensated. HF. Impaired renal function is an important
   prognostic indicator for adverse clinical outcome. Elevated plasma
   levels of adenosine have been observed in HF patients and stimulation of
   adenosine, receptors (A(1)R) in the kidney may be contributing to
   impaired renal function and treatment resistance. This observation has
   led to the development of AIR inhibitor drugs, both in oral and
   intravenous formulations, which in both animal and preliminary clinical
   trials have been shown to augment diuresis while preserving or improving
   renal function in HF patients. An extensive phase III clinical program
   using the AIR KW-3902 is now in progress in patients with symptomatic HF
   and renal dysfunction to evaluate the efficacy and safety of this
   treatment approach.}},
