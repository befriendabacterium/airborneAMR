@article{ ISI:000268436700012,
Author = {Thoen, Charles O. and LoBue, Philip A. and Enarson, Donald A. and
   Kaneene, John B. and de Kantor, Isabel N.},
Title = {{Tuberculosis: a re-emerging disease in animals and humans}},
