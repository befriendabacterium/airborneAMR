@article{ ISI:000282710700003,
Author = {LoBue, P. A. and Enarson, D. A. and Thoen, T. C.},
Title = {{Tuberculosis in humans and its epidemiology, diagnosis and treatment in
   the United States}},
Journal = {{INTERNATIONAL JOURNAL OF TUBERCULOSIS AND LUNG DISEASE}},
Year = {{2010}},
Volume = {{14}},
